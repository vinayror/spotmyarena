<section class="sub-banner">
            <!--Background-->
            <div class="bg-parallax bg-1"></div>
            <!--End Background-->
            <!-- Logo -->
            <div class="logo-banner text-center">
               
                <%= link_to root_path do %>
                    <%= image_tag "logo-banner.png", class: "" %>
                <% end %>
            </div>
            <!-- Logo -->
        </section>
        <!--End Banner-->

        <!-- Main -->
        <div class="main">
            <div class="container">
                <div class="main-cn element-page bg-white clearfix">
                    <!--Breakcrumb-->
                    <section class="breakcrumb-sc">
                        <ul class="breadcrumb arrow">
                            <li><a href="index.html"><i class="fa fa-home"></i></a></li>
                            <li>My Bookings</li>
                        </ul>
                    </section>
                    <!--End Breakcrumb-->
                    
                    <section class="user-profile">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="user-profile__header">
                                    <h4><%= current_user.name %></h4>
                                    <span>Member Since September 2015</span>
                                    <p>
                                        <%= image_tag('vinay.jpg') %>
                                    </p>
                                </div>
                                <ul class="user-profile__navigation">
                                    <% if current_user.has_role? :ground_owner || :admin%>
                                      <li class="current">
                                        <%= link_to grounds_path do %>
                                            My grounds<%= image_tag('icon-cart.png') %>
                                        <% end %>
                                      </li>
                                    <% else %>
                                      <li class="current">
                                        <%= link_to bookings_path do %>
                                            My bookings<%= image_tag('icon-cart.png') %>
                                        <% end %>
                                        </li>
                                    <% end %> 
                                    <li >
                                      <%= link_to profile_path, :class => 'navbar-link' do %>
                                        My profile <%= image_tag('icon-user.png') %>
                                      <% end %>
                                    </li>
                                    <li>
                                      <%= link_to edit_user_registration_path do %>
                                        Setting <%= image_tag('icon-setting.png') %>
                                      <% end %>
                                    </li>
                                    
                                    <li>
                                      <%= link_to destroy_user_session_path, method: :delete do %>
                                        logout <%= image_tag('icon-back.png') %>
                                      <% end %>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-md-9">
                                <h2 class="user-profile__title">My bookings</h2>

                                <div class="my-profile">

                                    <div class="row">
                                        <div class="col-md-6">
                                            <h4 class="my-profile__title"></h4> 
                                            <ul>
                                                
                                                
                                            </ul>   
                                        </div>

                                        <div class="col-md-6">
                                            <h4 class="my-profile__title"></h4> 
                                            <ul>
                                                
                                            </ul>   
                                        </div>
                                    </div>
                                    
                                    <p>
                                        
                                    </p>

                                    
                                    <%= link_to "go to home page", root_path ,  :class => 'awe-btn awe-btn-1 awe-btn-medium cancle' %>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </div>
        <!-- End Main -->